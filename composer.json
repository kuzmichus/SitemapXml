{
    "name": "kuzmichus/sitemap-xml",
    "description": "Generated Sitemap XML file",
    "minimum-stability": "dev",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/kuzmichus/SitemapXml",
    "authors": [
        {
            "name": "Sergey V. Kuzin",
            "email": "sergey@kuzin.name",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "SitemapXml\\": "library/"
        }
    },
    "suggest": {
        "ext-curl" : "It will check the availability of the resource to add to the sitemap.xml"
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
